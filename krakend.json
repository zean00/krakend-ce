{
    "version": 2,
    "extra_config": {
     "github_com/devopsfaith/krakend-gologging": {
        "level":  "DEBUG",
        "prefix": "[KRAKEND]",
        "syslog": false,
        "stdout": true
      }
    },
    "timeout": "3000ms",
    "cache_ttl": "300s",
    "output_encoding": "no-op",
    "name": "CoolUserService",
    "port": 8000,
    "endpoints": [
      {
        "endpoint": "/head",
        "method": "GET",
        "output_encoding": "no-op",
        "concurrent_calls": 1,
        "headers_to_pass": ["*"],
        "querystring_params": ["*"],
        "extra_config": {
          "github_com/zean00/jwtextract": {
            "extract_all" : false,
            "claim_map" : {
              "customer" : "subject",
              "account" : "account",
              "userid" : "phone_number"
            }
         }
        },
        "backend": [
          {
            "method": "GET",
            "host": [ "http://localhost:8000" ],
            "url_pattern": "/__debug/optional",
            "disable_host_sanitize": true,
            "encoding": "no-op",
            "sd": "static"
          }
        ]
      },
      {
        "endpoint": "/user/data",
        "method": "GET",
        "output_encoding": "no-op",
        "concurrent_calls": 1,
        "headers_to_pass": ["*"],
        "querystring_params": ["*"],
        "extra_config": {
          "github_com/zean00/kropa": {
            "service_address" : "http://localhost:8181",
            "package_name" : "dreams.user"
         }
        },
        "backend": [
          {
            "method": "GET",
            "host": [ "http://localhost:8080" ],
            "url_pattern": "/user/data",
            "disable_host_sanitize": true,
            "encoding": "no-op",
            "sd": "static"
          }
        ]
      }
    ]
  }